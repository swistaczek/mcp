{
  "mcpServers": {
    "Context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "env": {}
    },
    "Domain Checker": {
      "type": "stdio",
      "command": "sh",
      "args": ["-c", "uv run --with fastmcp fastmcp run domains.fastmcp.json"],
      "env": {}
    },
    "Gemini Alt Tag": {
      "type": "stdio",
      "command": "sh",
      "args": [
        "-c",
        "uv run --with fastmcp fastmcp run gemini_alt.fastmcp.json"
      ],
      "env": {
        "GEMINI_API_KEY": "your-gemini-api-key-here"
      }
    }
  }
}
